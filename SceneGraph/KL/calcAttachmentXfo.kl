use Mat44, Xfo;
use PoseVariables;


operator calcManipulatorAttachmentXfo(
  io Xfo localXfo,
  io PoseVariables poseVariables,
  io Size xfoIndex,
  io Xfo globalXfo
) {
  //report( "calcManipulatorAttachmentXfo\n" );
  globalXfo = poseVariables.xfoValues[xfoIndex] * localXfo;
//  globalXfo = poseVariables.xfoValues[xfoIndex];
}
