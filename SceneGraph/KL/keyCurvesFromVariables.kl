


use Vec2, KEYFRAMETRACKSETTYPE;
use LinearKeyframe;
use LinearKeyframeTrack;
use LinearKeyframeTrackSet;

operator keyCurvesFromVariables(
  io Scalar time,
  io PoseVariables poseVariables,
  io Integer boundTrackSet,
  io KEYFRAMETRACKSETTYPE trackSet<>,
  io KeyframeTrackBindings bindings<>
) {
  trackSet[boundTrackSet].setValues(
    poseVariables,
    time,
    bindings[boundTrackSet]
  );
}

