Import( 'extsEnv' )

tgaPluginEnv = extsEnv.Clone()
tgaPlugin = tgaPluginEnv.SharedLibrary( 'FabricTGA', Glob('*.cpp') ),
tgaManifest = tgaPluginEnv.File('FabricTGA.fpm.json'),
tgaFiles = [tgaManifest, tgaPlugin]

Return( 'tgaFiles' )
