struct Vec3
{
  Scalar x;
  Scalar y;
  Scalar z;
};

function Vec3( Scalar x, Scalar y, Scalar z )
{
  self.x = x;
  self.y = y;
  self.z = z;
}

function Vec3 +( Vec3 lhs, Vec3 rhs )
{
  var Vec3 result;
  result.x = lhs.x + rhs.x;
  result.y = lhs.y + rhs.y;
  result.z = lhs.z + rhs.z;
  return result;
}

function entry()
{
  var Vec3 lhs(1.0,2.0,3.0), rhs(4.0,5.0,6.0);
  report lhs + rhs;
}
